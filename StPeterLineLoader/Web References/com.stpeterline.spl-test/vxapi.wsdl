          <xs:element minOccurs="0" name="SailRefId" type="sch14:RecordId">
            <xs:annotation />
          </xs:element>
          <xs:element minOccurs="0" name="ShipCode" type="sch14:ShipCode">
            <xs:annotation />
          </xs:element>
          <xs:element name="DateTime" type="xs:dateTime">
            <xs:annotation />
          </xs:element>
          <xs:element minOccurs="0" name="Port" type="xs:string">
            <xs:annotation />
          </xs:element>
          <xs:element name="Pier" type="xs:string">
            <xs:annotation />
          </xs:element>
          <xs:element name="ActivityType" type="sch14:SailActivityType">
            <xs:annotation />
          </xs:element>
        </xs:sequence>
