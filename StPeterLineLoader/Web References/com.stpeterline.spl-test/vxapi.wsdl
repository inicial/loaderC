        <xs:annotation />
        <xs:sequence>
          <xs:element name="ShipCode" type="sch14:ShipCode">
            <xs:annotation />
          </xs:element>
          <xs:element name="RestaurantCode" type="sch14:Code">
            <xs:annotation />
          </xs:element>
          <xs:element name="TableNumber" type="xs:positiveInteger">
            <xs:annotation />
          </xs:element>
          <xs:element minOccurs="0" name="TableName">
            <xs:annotation />
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:maxLength value="40" />
                <xs:whiteSpace value="preserve" />
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="Capacity" type="xs:nonNegativeInteger">
            <xs:annotation />
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="CabinCapacity">
        <xs:annotation>
          <xs:documentation>
                Cabin capacity
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
          <xs:totalDigits value="5" />
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="AvailabilityResult">
        <xs:annotation>
          <xs:documentation>
                Availability result. OK - cabins and, possibly, guarantees are available, GTY- only guarantees are
                available, WTL - only waitlist
                is open, NO - either nothing is available or impossible to book there, for example,
                NO can be indicated for
                categories, where all cabins have capacity less than party size.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="sch14:InventoryResult" />
      </xs:simpleType>
      <xs:complexType name="InventoryUsage">
        <xs:annotation>
          <xs:documentation>
                Inventory usage totals
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="Result" type="sch14:AvailabilityResult" />
          <xs:element name="Abs" type="xs:integer">
            <xs:annotation>
              <xs:documentation>
                        Total number of requests with given inventory result in the related queue.
                    </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Wgt" type="xs:float">
            <xs:annotation>
              <xs:documentation>
                        Weighted number of requests.
                    </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="PriceItem">
        <xs:annotation>
          <xs:documentation>Individual price item</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="GuestSeqN" type="sch14:GuestSeqN" />
          <xs:element name="Code" type="sch14:Code">
            <xs:annotation>
              <xs:documentation>Price item code. Examples: 'VOYAGE FARE', 'INSURANCE', 'AIR SUPPLEMENT', etc. Also
                        see the PriceItemCode type description. Contains Invoice Item Type code if GroupingCode is 'nil'
                        in the input message.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Amount" type="sch14:Money">
            <xs:annotation>
              <xs:documentation>Always in the booking currency. If CombineGuests=Y contains total amount for all
                        guests in GuestRefs.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="AvailTravelClassCategory">
        <xs:sequence>
          <xs:element name="CabinCategory" type="sch14:TranslatedCode" />
          <xs:element name="Ship" type="sch14:Code" />
          <xs:element name="CabinCapacity" type="sch14:CabinCapacity" />
          <xs:element minOccurs="0" name="ChildBeds" type="xs:integer" />
          <xs:element minOccurs="0" name="Price">
            <xs:annotation>
              <xs:documentation>
                        This element is not present if CalcPrices was set to 'N' in the input message.
                    </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" name="Money" type="sch14:Money">
                  <xs:annotation>
                    <xs:documentation>
                                    Total price of the booking if this package/category is selected.
                                </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="Points" type="xs:float" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="Discount">
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" name="Money" type="sch14:Money" />
                <xs:element minOccurs="0" name="Points" type="xs:float" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="GuestPrices">
            <xs:annotation>
              <xs:documentation>
                        This element is not present if CalcPrices was set to 'N' in the input message.
                    </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="GuestPrice" type="sch14:PriceItem" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="Availability" type="sch14:AvailabilityResult">
            <xs:annotation>
              <xs:documentation>
                        Cumulative availability result for this category. Determined as worst availability result among
                        all resources, which will be
                        used if this category is booked. For example, for packages with mandatory hotel
                        components, if only certain hotels
                        are allowed for certain categories, unavailability of these hotels may result
                        in unavailability of these
                        categories.
                    </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="ShipAvailability">
            <xs:annotation>
              <xs:documentation>
                        Ship availability details.
                    </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:all>
                <xs:element name="NofCabins" type="xs:nonNegativeInteger">
                  <xs:annotation>
                    <xs:documentation>
                                    Total number of cabins in this category.
                                </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="AvailCabins">
                  <xs:annotation>
                    <xs:documentation>
                                    Number of unassigned cabins. Can be set to 99999, which means 'plenty of space',
                                    depending on
                                    cruise line security
                                    settings.
                                </xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                    <xs:restriction base="xs:integer">
                      <xs:totalDigits value="5" />
                    </xs:restriction>
                  </xs:simpleType>
                </xs:element>
                <xs:element minOccurs="0" name="NofReserved">
                  <xs:annotation>
                    <xs:documentation>
                                    Number of reserved cabins.
                                </xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                    <xs:restriction base="xs:integer">
                      <xs:totalDigits value="5" />
                    </xs:restriction>
                  </xs:simpleType>
                </xs:element>
                <xs:element minOccurs="0" name="AvailReserved">
                  <xs:annotation>
                    <xs:documentation>
                                    Number of unassigned reserved cabins.
                                </xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                    <xs:restriction base="xs:integer">
                      <xs:totalDigits value="5" />
                    </xs:restriction>
                  </xs:simpleType>
                </xs:element>
                <xs:element minOccurs="0" name="AvailabilityTotals" type="sch14:AvailabilityTotals" />
                <xs:element minOccurs="0" name="PartialSharedCabins">
                  <xs:annotation>
                    <xs:documentation>
                                    Availability of partial shared cabin
                                </xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element minOccurs="0" name="Common" type="xs:float">
                        <xs:annotation>
                          <xs:documentation>
                                                shared cabin "common"
                                            </xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="Male" type="xs:float">
                        <xs:annotation>
                          <xs:documentation>
                                                shared cabin "male"
                                            </xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="Female" type="xs:float">
                        <xs:annotation>
                          <xs:documentation>
                                                shared cabin "female"
                                            </xs:documentation>
                        </xs:annotation>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:all>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="CanBookCabins" type="xs:positiveInteger" />
          <xs:element minOccurs="0" name="ReserveType" type="sch14:TravelClassReserveType" />
          <xs:element minOccurs="0" name="SharedCabin">
            <xs:complexType>
              <xs:all>
                <xs:element name="Gender" type="sch14:GenderEnum" />
                <xs:element name="MaxOccupancy" type="sch14:Occupancy" />
              </xs:all>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="TravelClassReserveType">
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="15" />
          <xs:whiteSpace value="collapse" />
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="AvailabilityTotals">
        <xs:annotation>
          <xs:documentation>
                Inventory availability totals.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="Abs" type="xs:integer">
            <xs:annotation>
              <xs:documentation>
                        Total number of requests, which can be taken before absolute limits are exhausted. Can be
                        returned as 99999 instead of
                        actual value, which means 'plenty of space', depending on cruise line security
                        settings.
                    </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Wgt" type="xs:float">
            <xs:annotation>
              <xs:documentation>
                        Total number of requests, which can be taken before weighted limits are exhausted. Can be
                        returned as 99999 instead of
                        actual value, which means 'plenty of space', depending on cruise line security
                        settings.
                    </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="Referral">
        <xs:annotation>
          <xs:documentation>
                Cruise line specific referral information.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="0" name="Type" type="sch14:Code">
            <xs:annotation>
              <xs:documentation>
                        Examples: MAGAZINE, TV AD, MAILING, FRIEND, etc.
                    </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="Code" type="sch14:LongCode">
            <xs:annotation>
              <xs:documentation>
                        Can be used to specify exact referral source within its type. For example, for Type "MAGAZINE"
                        the Code value can be
                        "Money", "Newsweek", etc.
                    </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:element name="FaultDetail">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" name="Messages" type="sch14:Messages" />
          </xs:sequence>
        </xs:complexType>
