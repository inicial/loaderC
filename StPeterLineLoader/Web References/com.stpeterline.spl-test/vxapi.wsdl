<?xml version="1.0" encoding="utf-8"?>
<wsdl:definitions xmlns:sch17="http://www.versonix.com/vxapi/" xmlns:tns="http://www.versonix.com/vxapi" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:sch16="http://www.versonix.com/vxapi/" xmlns:sch11="http://www.versonix.com/vxapi/" xmlns:sch10="http://www.versonix.com/vxapi/" xmlns:sch13="http://www.versonix.com/vxapi/" xmlns:sch12="http://www.versonix.com/vxapi/" xmlns:sch0="http://www.versonix.com/vxapi/" xmlns:sch6="http://www.versonix.com/vxapi/" xmlns:sch8="http://www.versonix.com/vxapi/" xmlns:sch19="http://www.versonix.com/vxapi/" xmlns:sch18="http://www.versonix.com/vxapi/" xmlns:sch3="http://www.versonix.com/vxapi/" xmlns:sch4="http://www.versonix.com/vxapi/" xmlns:sch1="http://www.versonix.com/vxapi/" xmlns:sch21="http://www.versonix.com/vxapi/" xmlns:sch20="http://www.versonix.com/vxapi/" xmlns:sch5="http://www.versonix.com/vxapi/" xmlns:sch7="http://www.versonix.com/vxapi/" xmlns:sch9="http://www.versonix.com/vxapi/" xmlns:sch2="http://www.versonix.com/vxapi/" xmlns:sch15="http://www.versonix.com/vxapi/" xmlns:sch14="http://www.versonix.com/vxapi/" targetNamespace="http://www.versonix.com/vxapi" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">
  <wsdl:types>
    <xs:schema xmlns="http://www.versonix.com/vxapi/" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.versonix.com/vxapi/" xmlns:xs="http://www.w3.org/2001/XMLSchema">
      <xs:element name="GetAddonRequest">
        <xs:annotation>
          <xs:documentation>
                Returns the addon by code. The request must be send within active
                session and SessionGUID must be provided in request header.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent mixed="false">
            <xs:extension base="sch14:VXAPIRequest">
              <xs:sequence>
                <xs:element name="AddonCode" type="sch14:Code">
                  <xs:annotation>
                    <xs:documentation>
                                    Code of the retrieved addon
                                </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="IncludeClassificationsList" type="xs:boolean">
                  <xs:annotation>
                    <xs:documentation>
                                    Option to include the addon's classifications
                                </xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="GetAddonResponse">
        <xs:annotation>
          <xs:documentation>
                Response for GetAddonRequest.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent mixed="false">
            <xs:extension base="sch14:AbstractVXAPIResponse">
              <xs:sequence>
                <xs:element name="Addon">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element name="Code" type="sch14:TranslatedCode">
                        <xs:annotation>
                          <xs:documentation>
                                                Addon code
                                            </xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element name="Category" type="xs:string">
                        <xs:annotation>
                          <xs:documentation>
                                                Addon category
                                            </xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="Classifications">
                        <xs:annotation>
                          <xs:documentation>
                                                List of addon classifications
                                            </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element maxOccurs="unbounded" name="Code" type="sch14:LongCode" />
                          </xs:sequence>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="GetAvailableAddonsRequest">
        <xs:annotation>
          <xs:documentation>
				Returns the list of addons available for given reservation. The request must be send within active
				session and SessionGUID must be provided in request header.
			</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent mixed="false">
            <xs:extension base="sch14:VXAPIRequest">
              <xs:sequence>
                <xs:element name="ResId" type="sch14:RecordId">
                  <xs:annotation>
                    <xs:documentation>
									ID of the reservation
								</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="PackageId" type="sch14:RecordId">
                  <xs:annotation>
                    <xs:documentation>
									ID of the sail package
								</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:choice minOccurs="0">
                  <xs:element maxOccurs="unbounded" name="IncludeAddonCategoryElements" type="sch14:AddonCategoryElement">
                    <xs:annotation>
                      <xs:documentation>
										The list of elements will define which addons categories have to be included in
										search
										results.
									</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" name="ExcludeAddonCategoryElements" type="sch14:AddonCategoryElement">
                    <xs:annotation>
                      <xs:documentation>
										The list of elements will define which addons categories have to be excluded
										from search
										results.
									</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:choice>
              </xs:sequence>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="GetAvailableAddonsResponse">
        <xs:annotation>
          <xs:documentation>
				Response message for GetAvailableAddonsRequest.
			</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent mixed="false">
            <xs:extension base="sch14:AbstractVXAPIResponse">
              <xs:sequence>
                <xs:element minOccurs="0" name="Addons">
                  <xs:annotation>
                    <xs:documentation>
                                    List of addons
                                </xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="Addon" type="sch14:AvailAddon" />
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="AddAddonRequest">
        <xs:annotation>
          <xs:documentation>
				Adds addon request to the reservation.
			</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent mixed="false">
            <xs:extension base="sch14:VXAPIRequest">
              <xs:sequence>
                <xs:element name="ResId" type="sch14:RecordId">
                  <xs:annotation>
                    <xs:documentation>
                                    ID of the reservation
                                </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="Addons">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="Addon">
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element name="Code" type="sch14:Code">
                              <xs:annotation>
                                <xs:documentation>
                                                            Addon code
                                                        </xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="Quantity" type="xs:integer">
                              <xs:annotation>
                                <xs:documentation>
                                                            Quantity of addons of the given type
                                                        </xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="GuestRefs" type="sch14:GuestRefs">
                              <xs:annotation>
                                <xs:documentation>
                                                            GuestRefs, linked with the addon
                                                        </xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element minOccurs="0" name="LinkedRequest">
                              <xs:annotation>
                                <xs:documentation>
                                                            Entity which the addon is linked with
                                                        </xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                <xs:choice>
                                  <xs:element name="DiningReqId" type="sch14:RecordId" />
                                  <xs:element name="RoomSeqN" type="sch14:SeqN" />
                                  <xs:element name="PackageId" type="sch14:RecordId" />
                                  <xs:element name="ShipRoomReqId" type="sch14:RecordId" />
                                  <xs:element name="CarDeckRequest">
                                    <xs:complexType>
                                      <xs:sequence>
                                        <xs:element name="CarDeckReqId" type="sch14:RecordId" />
                                        <xs:element name="LegSeqN" type="sch14:SeqN" />
                                      </xs:sequence>
                                    </xs:complexType>
                                  </xs:element>
                                </xs:choice>
                              </xs:complexType>
                            </xs:element>
                          </xs:sequence>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="AddAddonResponse">
        <xs:annotation>
          <xs:documentation>
				Response message for AddAddonRequest.
			</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent mixed="false">
            <xs:extension base="sch14:AbstractReservationResponse" />
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="RemoveAddonRequest">
        <xs:annotation>
          <xs:documentation>
				Removes addon from the reservation. The request must be executed in the session.
			</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent mixed="false">
            <xs:extension base="sch14:VXAPIRequest">
              <xs:sequence>
                <xs:element name="ResId" type="sch14:RecordId">
                  <xs:annotation>
                    <xs:documentation>
                                    ID of the reservation
                                </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="AddonIds">
                  <xs:annotation>
                    <xs:documentation>
                                    List of addon IDs to be removed
                                </xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="AddonId" type="sch14:RecordId" />
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="RemoveAddonResponse">
        <xs:annotation>
          <xs:documentation>
				Response message for RemoveAddonRequest
			</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent mixed="false">
            <xs:extension base="sch14:AbstractReservationResponse" />
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:complexType name="PriceList">
        <xs:all>
          <xs:element name="Date" type="xs:date" />
          <xs:element name="Price" type="sch14:Money" />
          <xs:element minOccurs="0" name="Currency" type="sch14:CurrencyEnum" />
        </xs:all>
      </xs:complexType>
      <xs:complexType name="CarCategory">
        <xs:sequence>
          <xs:element name="CarPriceCategory" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="CreditCardLastDigits">
        <xs:restriction base="xs:integer">
          <xs:totalDigits value="4" />
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="Departure">
        <xs:sequence>
          <xs:element name="ArrivalCity" type="sch14:CityCode" />
          <xs:element name="DepartureCity" type="sch14:CityCode" />
          <xs:element name="DepartureDate" type="sch14:DateRange" />
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="RecordId">
        <xs:annotation>
          <xs:documentation>
                Uniquely identifies record in the set of similar records. If positive - refers to an existing
                record in Seaware database, it
                negative - refers to (or defines) a record inside the XML document.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
          <xs:totalDigits value="9" />
          <xs:fractionDigits value="0" />
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="SeqN">
        <xs:annotation>
          <xs:documentation>
                Sequence number of the element in the list of elements of same types in a booking.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:positiveInteger">
          <xs:minInclusive value="1" />
          <xs:whiteSpace value="collapse" />
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="Email">
        <xs:restriction base="xs:string">
          <xs:pattern value=".+@.+[.]+.+" />
          <xs:minLength value="1" />
          <xs:maxLength value="80" />
          <xs:whiteSpace value="collapse" />
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="RewardNumber">
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="20" />
          <xs:whiteSpace value="collapse" />
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="GUID">
        <xs:annotation>
          <xs:documentation>
                Globally unique identifier. Format: HHHHHHHH-HHHH-HHHH-HHHH-HHHHHHHHHHHH, where H in '0'..'9',
                'a'..'f', 'A'..'F'.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:minLength value="36" />
          <xs:maxLength value="36" />
          <xs:whiteSpace value="collapse" />
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="MsgHeader">
        <xs:annotation>
          <xs:documentation>
                The header element of a request to VXAPI or VXAPI's response
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="0" name="Language" type="sch14:LanguageEnum">
            <xs:annotation>
              <xs:documentation>
                        If defined, the response object will contain translations in this language. Please note, that
                        this parameter is
                        mandatory for some requests. This is highlighted in the description of the requests.
                    </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="Country" type="sch14:CountryEnum">
            <xs:annotation>
              <xs:documentation>
                        If defined, the response object will contain content specific for given country. Please note,
                        that this parameter is
                        mandatory for some requests (for example for requests for loading content from CMS). This
                        is highlighted in the
                        description of the requests.
                    </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="SessionGUID" type="sch14:GUID">
            <xs:annotation>
              <xs:documentation>
                        Session GUID must be included into the header of the requests executed within session. Usually
                        that are requests for
                        reservation management, such as reservation update, passengers list update etc.
                    </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="CallerInfo" type="sch14:CallerInfo">
            <xs:annotation>
              <xs:documentation>
                        Not used in outgoing messages
                    </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="MessageId" type="sch14:MessageId">
            <xs:annotation>
              <xs:documentation>
                        An arbitrary request identification, which will be echoed back in the response message.
                    </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="MessageGroupCode">
        <xs:annotation>
          <xs:documentation>
                Message group code
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="30" />
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="MessageCode">
        <xs:annotation>
          <xs:documentation>
                Message code
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer" />
      </xs:simpleType>
      <xs:complexType name="Message">
        <xs:annotation>
          <xs:documentation>
                Informational or warning message returned by the system
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="0" name="Group" type="sch14:MessageGroupCode" />
          <xs:element name="Code" type="sch14:MessageCode" />
          <xs:element name="Message">
            <xs:annotation>
              <xs:documentation>
                        Text of the message
                    </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:minLength value="1" />
                <xs:whiteSpace value="preserve" />
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="Severity" type="sch14:MessageSeverity" />
          <xs:element name="Source" type="sch14:MessageSource" />
          <xs:element minOccurs="0" name="Advice">
            <xs:annotation>
              <xs:documentation>
                        Advice of how to handle the exception or error
                    </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:minLength value="1" />
                <xs:whiteSpace value="preserve" />
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="0" name="BookingValidation">
            <xs:annotation>
              <xs:documentation>
                        Additional information that can be provided for booking validation
                    </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" name="CanBeStored" type="xs:boolean">
                  <xs:annotation>
                    <xs:documentation>
                                    Identifies if the reservation with this message can be stored
                                </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="OnStoreOnly" type="xs:boolean">
                  <xs:annotation>
                    <xs:documentation>
                                    Identifies if the message appears only when the reservation is stored
                                </xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="Messages">
        <xs:annotation>
          <xs:documentation>
                List of informational and warning messages
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="Message" type="sch14:Message" />
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="Code">
        <xs:annotation>
          <xs:documentation>
                Generic code (string(15)).
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="15" />
          <xs:whiteSpace value="collapse" />
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="LongCode">
        <xs:annotation>
          <xs:documentation>
                Generic code (string(30)).
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="30" />
          <xs:whiteSpace value="collapse" />
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="OfficeCode">
        <xs:annotation>
          <xs:documentation>
                Office Code - office location identifier (for example, 'MIAMI HQ', 'LONDON', etc.)
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="sch14:Code" />
      </xs:simpleType>
      <xs:simpleType name="Username">
        <xs:annotation>
          <xs:documentation>
                User name for logging in.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="80" />
          <xs:whiteSpace value="collapse" />
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="Password">
        <xs:annotation>
          <xs:documentation>
                Password.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="30" />
          <xs:whiteSpace value="collapse" />
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="FirstName">
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="30" />
          <xs:whiteSpace value="collapse" />
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="LastName">
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="30" />
          <xs:whiteSpace value="collapse" />
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="FullName">
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:whiteSpace value="collapse" />
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="PreferenceLanguage">
        <xs:restriction base="xs:string">
          <xs:minLength value="3" />
          <xs:maxLength value="3" />
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="WEB">
        <xs:all>
          <xs:element name="AllowAccess" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                        Option to allow the WEB access
                    </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="Username" type="sch14:Username" />
        </xs:all>
      </xs:complexType>
      <xs:complexType name="MarketingAllowed">
        <xs:annotation>
          <xs:documentation>
                Options for enabling EMails, Mails or SMSs for marketing purposes
            </xs:documentation>
        </xs:annotation>
        <xs:all>
          <xs:element name="Mail" type="xs:boolean" />
          <xs:element name="Email" type="xs:boolean" />
          <xs:element name="SMS" type="xs:boolean" />
        </xs:all>
      </xs:complexType>
      <xs:simpleType name="Hobbies">
        <xs:list itemType="sch14:Hobby" />
      </xs:simpleType>
      <xs:complexType name="ClientInfo">
        <xs:all>
          <xs:element name="ClientId" type="sch14:RecordId" />
          <xs:element name="ProgramCode" type="sch14:Code">
            <xs:annotation>
              <xs:documentation>
                        Examples: 'AA Mileage', 'UA Mileage', etc.
                    </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="ActiveFrom">
            <xs:simpleType>
              <xs:restriction base="xs:date">
                <xs:whiteSpace value="collapse" />
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="0" name="ActiveTo">
            <xs:simpleType>
              <xs:restriction base="xs:date">
                <xs:whiteSpace value="collapse" />
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
        </xs:all>
      </xs:complexType>
      <xs:complexType name="TravelClass">
        <xs:annotation>
          <xs:appinfo>
            <jaxb:property xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" name="TravelClass" />
          </xs:appinfo>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="SeqN" type="sch14:SeqN" />
          <xs:element name="PriceCategory" type="sch14:TranslatedCode" />
          <xs:element minOccurs="0" name="Gender" type="sch14:GenderEnum" />
          <xs:element minOccurs="0" name="MaxOccupancy" type="sch14:Occupancy" />
          <xs:element minOccurs="0" name="ReserveType" type="sch14:TravelClassReserveType" />
          <xs:element name="GuestRefs" type="sch14:GuestRefs" />
          <xs:element minOccurs="0" name="ChildBeds" type="sch14:GuestRefs" />
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="Vehicle">
        <xs:sequence>
          <xs:element name="RequestId" type="sch14:RecordId" />
          <xs:element name="Code" type="sch14:TranslatedCode" />
          <xs:element minOccurs="0" name="LicensePlate" type="sch14:LicensePlate" />
          <xs:element minOccurs="0" name="LicensePlateCountry" type="sch14:CountryEnum" />
          <xs:element minOccurs="0" name="Trailer">
            <xs:complexType>
