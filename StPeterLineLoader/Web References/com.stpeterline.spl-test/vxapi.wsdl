                            <xs:element maxOccurs="unbounded" name="HotelRoom">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="Quantity" type="xs:positiveInteger" />
                                  <xs:element name="PriceCategory" type="sch14:TranslatedCode" />
                                  <xs:element minOccurs="0" name="Occupancy" type="xs:positiveInteger" />
                                  <xs:element minOccurs="0" name="Comments" />
                                  <xs:element name="Price" type="sch14:Money" />
                                  <xs:element name="GuestCategories">
                                    <xs:complexType>
                                      <xs:sequence>
                                        <xs:element maxOccurs="unbounded" name="GuestCategory">
                                          <xs:complexType>
                                            <xs:sequence>
                                              <xs:element name="Quantity" type="xs:positiveInteger" />
                                              <xs:element name="PriceCategory" type="sch14:TranslatedCode" />
                                              <xs:element name="Price" type="sch14:Money" />
                                            </xs:sequence>
                                          </xs:complexType>
                                        </xs:element>
                                      </xs:sequence>
                                    </xs:complexType>
                                  </xs:element>
                                </xs:sequence>
